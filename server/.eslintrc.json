{
  "extends": "standard",
  "installedESLint": true,
  "plugins": [
    "standard",
    "promise"
  ],
  "rules": {
    "indent": [
      2,
      4
    ],
    "semi": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "eol-last": [
      2,
      "never"
    ]
  }
}